{
  "switches": {
    "sw1": {
      "class": "example.maclearning.MacLearningSwitch",
      "prog": "example/l2switch.p4",
      "enable_ctrl": true,
      "ctrl_args": {
        "ctrl_port": 1,
        "start_wait": 0.5
      }
    },
    "sw2": {
      "class": "example.maclearning.MacLearningSwitch",
      "prog": "/p4app/example/l2switch.p4",
      "enable_ctrl": true,
      "ctrl_args": {
        "ctrl_port": 1,
        "start_wait": 0.5
      }
    }
  },
  "hosts": {
    "h1": { "ip": "10.0.0.1" },
    "h2": { "ip": "10.0.0.2" },
    "h3": { "ip": "10.0.0.3" }
  },
  "links": [
    [ "h1", "sw1" ],
    [ "h2", "sw2" ],
    [ "h3", "sw2" ],
    [ "sw1", "sw2" ]
  ]
}